          <th class="last"></th>
        </tr>
      </thead>
      <tbody>
        @foreach (DashboardProcessedViewModel dashboardProcessedViewModel in Model.DashboardProcessedViewModels)
        {
          <tr>
            <td colspan="5">
              <table class="subtable">
                <thead>
                  <tr>
                    <th class="data5 odd">@Html.ActionLink(@dashboardProcessedViewModel.Grid, "SearchGrid", "Document", new { grid = dashboardProcessedViewModel.Grid }, null)
                    </th>
                    <th class="data4 odd">@dashboardProcessedViewModel.DocType</th>
                    <th class="data5 odd">@dashboardProcessedViewModel.ManCo</th>
                    <th class="data5 odd">@dashboardProcessedViewModel.StartDate</th>
                    <th class="last"><span class="moreinfo">+ more info</span></th>
