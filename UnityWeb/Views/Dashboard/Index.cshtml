@using UnityWeb.File
@using UnityWeb.Models.Dashboard
@using UnityWeb.Resources.Dashboard
@model DashboardViewModel
@{
    ViewBag.Title = Index.Dashboard;
}

<div class="clearfix"></div>
<section class="accordion accordionDash">
      <div>                   
             @Html.Action("ManCoDropDown", "Dashboard")
      </div>
    <h3><a>@Index.ProcessingNow</a><span class="floatright">click to expand</span></h3>
    <section class="pane">
        <table>
            <thead>
                <tr>
                    <th class="data5">GRID</th>
                    <th class="data4">DOC TYPE</th>
                    <th class="data5">MAN CO</th>
                    <th class="data5">PROCESSING START</th>
                    <th class="last"></th>
                </tr>
            </thead>
            <tbody>
                @foreach (DashboardProcessingViewModel dashboardProcessingViewModel in Model.DashboardProcessingViewModels)
                {
                    <tr>
                        <td colspan="5">
                            <table class="subtable">
                                <thead>
                                    <tr>
                                        <th class="data5 odd">@dashboardProcessingViewModel.Grid</th>
                                        <th class="data4 odd">@dashboardProcessingViewModel.DocType</th>
                                        <th class="data5 odd">@dashboardProcessingViewModel.ManCo</th>
                                        <th class="data5 odd">@dashboardProcessingViewModel.StartDate</th>
                                        <th class="last"><span class="moreinfo">+ more info</span></th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td colspan="6">
                                            <p>
                                                <strong>BIG ZIP:</strong>
                                                @dashboardProcessingViewModel.BigZip
                                                <br />
                                                <strong>INPUT XML:</strong>
                                                @dashboardProcessingViewModel.FileName
                                            </p>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </section>
</section>
<section class="accordion accordionDash">
  <h3><a>@Index.RecentlyCompleted</a><span class="floatright">click to expand</span></h3>
