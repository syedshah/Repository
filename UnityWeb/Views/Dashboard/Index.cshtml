                      <p>
                        <strong>BIG ZIP:</strong>
                        @dashboardProcessedViewModel.BigZip
                        <br />
                        <strong>INPUT XML:</strong>
                        @dashboardProcessedViewModel.FileName
                      </p>
                    </td>
                  </tr>
                </tbody>
              </table>
            </td>
          </tr>
        }
      </tbody>
    </table>
    <div class="clearfix"></div>
  </section>
</section>
